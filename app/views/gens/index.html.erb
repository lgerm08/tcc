<p id="notice"><%= notice %></p>

<h1>Gens</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Pf</th>
      <th>Sn</th>
      <th>Un</th>
      <th>If0</th>
      <th>Uf0</th>
      <th>Ifn</th>
      <th>Ufn</th>
      <th>Xd</th>
      <th>Xq</th>
      <th>Ra</th>
      <th>Pferro</th>
      <th>Pmec</th>
      <th>Psup</th>
      <th>Pven</th>
      <th>K exm</th>
      <th>K prl</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gens.each do |gen| %>
      <tr>
        <td><%= gen.name %></td>
        <td><%= gen.PF %></td>
        <td><%= gen.SN %></td>
        <td><%= gen.UN %></td>
        <td><%= gen.If0 %></td>
        <td><%= gen.Uf0 %></td>
        <td><%= gen.Ifn %></td>
        <td><%= gen.Ufn %></td>
        <td><%= gen.Xd %></td>
        <td><%= gen.Xq %></td>
        <td><%= gen.ra %></td>
        <td><%= gen.Pferro %></td>
        <td><%= gen.Pmec %></td>
        <td><%= gen.Psup %></td>
        <td><%= gen.Pven %></td>
        <td><%= gen.k_exm %></td>
        <td><%= gen.k_prl %></td>
        <td><%= link_to 'Show', gen %></td>
        <td><%= link_to 'Edit', edit_gen_path(gen) %></td>
        <td><%= link_to 'Destroy', gen, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gen', new_gen_path %>
